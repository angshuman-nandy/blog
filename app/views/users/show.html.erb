<% if logged_in? %>

  <% provide(:title, @user.email) %>
  <div class="row">
    <aside class="col-md-4">
      <section class="user_info">
        <h1>
         <%= gravatar_for @user %>
          <%= @user.email %>
          <%= @user.password_digest %>
        </h1>
      </section>
    </aside>
  </div>
<%= link_to "Log out", logout_path, method: :delete %>
<% end %>